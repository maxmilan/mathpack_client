$(function () {
  new Highcharts.Chart({
    chart: { renderTo: 'function_plot' },
    title: { text: '<%= function_name %>' },
    xAxis: { tickInterval: <%= tick %> },
    yAxis: {
      title: { text: 'y'}
    },
    series: [{
      data: <%= data %>,
      pointStart: 0,
      name: '<%= function_name %>'
    }]
  });
});
